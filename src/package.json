{
  "name": "umihiko-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production forever start watch.js",
    "build": "yarn clean && yarn esbuild",
    "dev": "NODE_ENV=development yarn build && yarn nodemon ./dist/bin/www",
    "clean": "rimraf dist",
    "watch": "node watch.js",
    "babel": "babel ./server --out-dir dist",
    "esbuild": "node esbuild.js",
    "rollup": "rollup -c"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^4.5.0",
    "axios": "^0.21.0",
    "chokidar": "^3.4.3",
    "cognito-express": "^2.0.18",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "forever": "^3.0.2",
    "graphql": "^15.3.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pg": "^8.4.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "dbdocs": "^0.3.1",
    "esbuild": "^0.7.19",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2"
  }
}
