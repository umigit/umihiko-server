{
  "name": "umihiko-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production forever start ./dist/bin/www",
    "build": "yarn clean && yarn esbuild",
    "dev": "NODE_ENV=development yarn build && yarn nodemon ./dist/bin/www",
    "lint": "yarn eslint && yarn tsc --noEmit",
    "lint:fix": "yarn eslint && yarn tsc --noEmit && yarn format",
    "clean": "rimraf dist",
    "watch": "ts-node watch.ts",
    "babel": "babel ./server --out-dir dist",
    "esbuild": "ts-node esbuild.ts",
    "eslint": "eslint ./**/*.ts",
    "eslint:fix": "eslint ./**/*.ts --fix",
    "rollup": "rollup -c",
    "format": "prettier --write './**/*.{js,ts,json}'"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "axios": "^0.21.0",
    "chokidar": "^3.4.3",
    "cognito-express": "^2.0.18",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "forever": "^3.0.2",
    "http-errors": "~1.8.0",
    "morgan": "~1.10.0",
    "pg": "^8.6.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.34",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.2",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "dbdocs": "^0.5.0",
    "dts-gen": "^0.6.0",
    "esbuild": "^0.11.18",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
