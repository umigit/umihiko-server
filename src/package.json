{
  "name": "umihiko-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "node ./dist/bin/www",
    "build": "yarn clean && yarn babel",
    "dev": "NODE_ENV=development yarn build && yarn watch",
    "clean": "rimraf dist",
    "watch": "nodemon ./dist/bin/www",
    "babel": "babel ./server --out-dir dist"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pg": "^8.4.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "nodemon": "^2.0.5",
    "rimraf": "^3.0.2"
  }
}
